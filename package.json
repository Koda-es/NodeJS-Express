{
  "name": "nodejs_express_mysql",
  "version": "1.0.0",
  "description": "NodeJS Express MySQL JS Backend",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js",
    "test": "jest --verbose",
    "test:watch": "npm run test -- --watch",
    "dev": "set NODE_ENV=development&& node server.js",
    "prod": "set NODE_ENV=production&& node server.js"
  },
  "keywords": [],
  "author": "Adur Marques",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "5.0.1",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "mysql": "^2.18.1",
    "node-input-validator": "^4.4.1",
    "validator": "^13.6.0"
  },
  "devDependencies": {
    "eslint": "7.32.0",
    "jest": "27.0.6",
    "nodemon": "^2.0.12",
    "supertest": "6.1.4"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json",
    "env": {
      "jest": true
    }
  },
  "jest": {
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@controllers(.*)$": "<rootDir>/src/controllers$1",
      "^@models(.*)$": "<rootDir>/src/models$1",
      "^@routes(.*)$": "<rootDir>/src/routes$1",
      "^@validators(.*)$": "<rootDir>/src/validators$1",
      "^@config(.*)$": "<rootDir>/config$1"
    }
  },
  "_moduleAliases": {
    "@controllers": "./src/controllers",
    "@models": "./src/models",
    "@routes": "./src/routes",
    "@validators": "./src/validators",
    "@config": "./config"
  }
}
